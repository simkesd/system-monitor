{
    "env": {
      "node": true,
      "es2021": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb-base",
      "plugin:node/recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": ["node"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "node/no-unpublished-require": "off",
      "no-console": "off",
      "node/no-unsupported-features/es-syntax": "off", 
      "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]
    }
  }
  